{
  "entrypoint": "index.html",
  "shell": "src/dylan-app.html",
  "fragments": [
    "src/dylan-home.html",
    "src/dylan-all.html",
    "src/dylan-contact.html",
    "src/dylan-designer.html",
    "src/dylan-developer.html",
    "src/dylan-personal.html",
    "src/dylan-social.html",
    "src/dylan-404.html",
    "src/element-card.html",
    "src/home-card.html",
    "src/dylan-icons.html",
    "src/shared-styles.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "data/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
